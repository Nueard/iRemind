<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.iremind" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>iRemind</name>
    <description>
        Description.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        iRemind Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*.googleapis.com/*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <plugin name="cordova-plugin-geolocation" spec="~1.0.1" />
    <plugin name="cordova-sqlite-storage" spec="https://github.com/litehelpers/Cordova-sqlite-storage.git" />
    <plugin name="de.appplant.cordova.plugin.local-notification" spec="~0.8.2" />
    <plugin name="cordova-plugin-vibration" spec="~2.0.0" />
    <plugin name="de.appplant.cordova.plugin.background-mode" spec="https://github.com/katzer/cordova-plugin-background-mode.git" />
    <plugin name="cordova-plugin-geofence" spec="~0.5.0" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.1" />
</widget>
